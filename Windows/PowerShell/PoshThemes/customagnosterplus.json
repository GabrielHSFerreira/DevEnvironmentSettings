{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "right",
      "segments": [
        {
          "foreground": "#eceff4",
          "properties": {
            "time_format": "15:04:05"
          },
          "style": "plain",
          "type": "time"
        }
      ],
      "type": "prompt",
      "vertical_offset": -1
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "background": "#81a1c1",
          "foreground": "#eceff4",
          "powerline_symbol": "",
          "style": "powerline",
          "type": "session",
          "properties": {
            "default_user_name": "Gabriel",
            "display_default": false
          }
        },
        {
          "background": "#81a1c1",
          "foreground": "#eceff4",
          "powerline_symbol": "",
          "properties": {
            "folder_icon": "",
            "folder_separator_icon": "  ",
            "home_icon": "",
            "style": "agnoster"
          },
          "style": "powerline",
          "type": "path"
        },
        {
          "background": "#a3be8c",
          "foreground": "#2e3440",
          "powerline_symbol": "",
          "style": "powerline",
          "type": "git",
          "properties": {
            "display_status": true,
            "display_status_detail": true,
            "display_stash_count": true,
            "display_worktree_count": true,
            "status_colors_enabled": true,
            "local_changes_color": "#bf616a",
            "behind_color": "#b48ead",
            "ahead_color": "#5e81ac",
            "ahead_and_behind_color": "#b48ead"
          }
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "console_title": true
}
